%h2 Contact Us
%table.semantic.fullwidth
  %thead
    %tr
      %th Position
      %th Name
      %th Email
      %th Phone
  %tbody
    - @user_jobs.each do |uj|
      %tr
        %td= uj.job.jobname
        %td= uj.user.primary_attendee.get_full_name
        %td= uj.user.email
        %td
%dl
  %dt TC
  %dd Tournament Coordinator
  %dt TD
  %dd Tournament Director
